    agent any

    stages {
        stage('Build') {
            steps {
                echo 'this is build stage......'
            }
        }
        stage('Deploy') {
            steps {
                echo 'this is deploy stage......'
            }
        }
        stage('Test') {
            steps {
                echo 'this is test stage......'
            }
        }
      stage('Release') {
            steps {
                echo 'this is release stage......'
            }
        }
    }
}
